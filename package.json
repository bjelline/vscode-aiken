{
    "name": "vscode-gift",
    "displayName": "GIFT Language",
    "description": "Syntax highlighting and snippets for Moodle's GIFT format",
    "version": "0.0.3",
    "publisher": "ethan-ou",
    "engines": {
        "vscode": "^1.40.0"
    },
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/ethan-ou/vscode-gift"
    },
    "keywords": [
        "gift",
        "moodle"
      ],
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "gift",
            "aliases": ["Gift", "gift"],
            "extensions": [".txt", ".gift"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "gift",
            "scopeName": "source.gift",
            "path": "./syntaxes/gift.tmLanguage.json"
        }],
        "snippets": [
            {
                "language": "gift",
                "path": "./snippets/gift.json"
            }
        ]
    }
}