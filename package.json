{
    "name": "vscode-gift",
    "displayName": "vscode-gift",
    "description": "Syntax highlighting for Moodle's GIFT format",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.40.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "gift",
            "aliases": ["gift", "gift"],
            "extensions": [".txt"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "gift",
            "scopeName": "source.txt",
            "path": "./syntaxes/gift.tmLanguage.json"
        }]
    }
}